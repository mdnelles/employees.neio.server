{
   "name": "nodejs-server-employees",
   "version": "1.0.0",
   "description": "Server side application for employees app",
   "engines": {
      "node": "^18.0.0",
      "npm": "^8.0.0"
   },
   "main": "server.ts",
   "scripts": {
      "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save",
      "client-install": "npm install --prefix client",
      "start": "node server.ts",
      "server": "nodemon server.ts --ignore client"
   },
   "adminStart": true,
   "keywords": [
      "nodejs",
      "jwt",
      "passport",
      "express"
   ],
   "author": "mdnelles",
   "license": "ISC",
   "dependencies": {
      "array-sort": "^1.0.0",
      "bcrypt": "^5.0.1",
      "body-parser": "^1.20.0",
      "concurrently": "*",
      "cookie-parser": "^1.4.6",
      "cors": "*",
      "dotenv": "^16.0.1",
      "express": "*",
      "express-fileupload": "^1.4.0",
      "express-session": "^1.17.3",
      "flatted": "^3.2.6",
      "formidable": "^2.0.1",
      "fs-extra": "^10.1.0",
      "helmet": "^5.1.1",
      "jquery": "^3.6.0",
      "jsonwebtoken": "^8.5.1",
      "morgan": "^1.10.0",
      "mysql2": "^2.3.3",
      "nodemailer": "^6.7.7",
      "path": "^0.12.7",
      "sequelize": "^6.21.3",
      "serve": "^14.0.1",
      "shelljs": "^0.8.5",
      "ts-node": "^10.9.1",
      "uuid": "^8.3.2"
   },
   "devDependencies": {

{
   "name": "nodejs-server-employees",
   "version": "0.1.08",
   "description": "Server side application for employees app",
   "engines": {
      "node": "^16.16.0",
      "npm": "^8.15.1"
   },
   "main": "./src/server.ts",
   "scripts": {
      "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save",
      "client-install": "npm install --prefix client",
      "start": "cp ./env/env-dev ./.env && ts-node ./src/server.ts",
      "dockerize": "cp ./env/env-docker ./.env && ts-node ./src/server.ts ",
      "server": "cp ./env/env-dev ./.env && nodemon ./src/server.ts --ignore client ",
      "format": "prettier --write **/*.ts",
      "format-check": "prettier --check **/*.ts",
      "test": "jest --coverage",
      "pre-checkin": "npm run format && npm run build && npm test"
   },
   "adminStart": true,
   "keywords": [
      "nodejs",
      "jwt",
      "passport",
      "express"
   ],
   "author": "mdnelles",
   "license": "ISC",
   "dependencies": {
      "@actions/github": "^1.1.0",
      "@types/express": "^4.17.13",
      "array-sort": "^1.0.0",
      "bcrypt": "^5.0.1",
      "body-parser": "^1.20.0",
      "cors": "*",
      "dotenv": "^16.0.1",
      "errorhandler": "^1.5.1",
      "express": "*",
      "express-session": "^1.17.3",
      "flatted": "^3.2.6",
      "formidable": "^2.0.1",
      "fs-extra": "^10.1.0",
      "helmet": "^5.1.1",
      "jsonwebtoken": "^8.5.1",
      "mysql2": "^2.3.3",
      "path": "^0.12.7",
      "sequelize": "^6.21.3",
      "serve": "^14.0.1",
      "shelljs": "^0.8.5",
      "ts-node": "^10.9.1",
      "uuid": "^8.3.2"
   },
   "devDependencies": {
      "@types/bcrypt": "^5.0.0",
      "@types/body-parser": "^1.19.2",
      "@types/compression": "^1.7.2",
      "@types/cors": "^2.8.12",
      "@types/express": "^4.17.13",
      "@types/express-session": "^1.17.5",
      "@types/jest": "^27.0.2",
      "@types/jsonwebtoken": "^8.5.8",
      "@types/node": "^18.6.3",
      "jest": "^27.2.5",
      "jest-circus": "^27.2.5",
      "prettier": "^1.19.1",
      "ts-jest": "^27.0.5",
      "nodemon": "^2.0.19",
      "tslib": "^2.4.0",
      "typescript": "^3.8.3"
   }
}
